{
    "bindings": {
        "/actions/main": {
            "poses": [
                {
                    "output": "/actions/main/in/PoseLeft",
                    "path": "/user/hand/left/input/pose"
                },
                {
                    "output": "/actions/main/in/PoseRight",
                    "path": "/user/hand/right/input/pose"
                }
            ],
            "sources": [
                {
                    "path": "/user/hand/left/input/thumbstick",
                    "mode": "joystick",
                    "inputs": {
                        "position": {
                            "output": "/actions/main/in/SelectLeft"
                        }
                    }
                },
                {
                    "path": "/user/hand/right/input/thumbstick",
                    "mode": "joystick",
                    "inputs": {
                        "position": {
                            "output": "/actions/main/in/SelectRight"
                        }
                    }
                },
                {
                    "path": "/user/hand/left/input/a",
                    "mode": "button",
                    "inputs": {
                        "pull": {
                            "output": "/actions/main/in/OpenLeft"
                        },
                        "click": {
                            "output": "/actions/main/in/openleft"
                        }
                    }
                },
                {
                    "path": "/user/hand/right/input/a",
                    "mode": "button",
                    "inputs": {
                        "pull": {
                            "output": "/actions/main/in/OpenRight"
                        },
                        "click": {
                            "output": "/actions/main/in/openright"
                        }
                    }
                },
                {
                    "path": "/user/hand/left/input/trigger",
                    "mode": "button",
                    "inputs": {
                        "click": {
                            "output": "/actions/main/in/ClickLeft"
                        }
                    }
                },
                {
                    "path": "/user/hand/right/input/trigger",
                    "mode": "button",
                    "inputs": {
                        "click": {
                            "output": "/actions/main/in/ClickRight"
                        }
                    }
                }
            ]
        }
    },
    "controller_type": "knuckles",
    "description": "",
    "name": "binding_knuckles_default"
}